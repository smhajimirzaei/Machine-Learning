saveLoadpath = '/content/drive/MyDrive/RTL_Lubricity/Machine_Vision/Hucklebery/6_8_2023/Test_1/'

from keras.models import load_model
Imported_model = load_model(saveLoadpath + 'Test_1_A.h5')
